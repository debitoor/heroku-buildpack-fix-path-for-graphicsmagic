#!/usr/bin/env bash

#!/usr/bin/env bash
[ -z "${MAGICK_CONFIGURE_PATH}" ] && export MAGICK_CONFIGURE_PATH=/app/.apt/usr/lib/GraphicsMagick-1.3.23/config && echo "fixing MAGICK_CONFIGURE_PATH"
[ -z "${MAGICK_CODER_MODULE_PATH}" ] && export MAGICK_CODER_MODULE_PATH=/app/.apt/usr/lib/GraphicsMagick-1.3.23/modules-Q8/coders && echo "fixing MAGICK_CODER_MODULE_PATH"
[ -z "${MAGICK_FILTER_MODULE_PATH}" ] && export MAGICK_FILTER_MODULE_PATH=/app/.apt/usr/lib/GraphicsMagick-1.3.23/modules-Q8/filters && echo "fixing MAGICK_FILTER_MODULE_PATH"
